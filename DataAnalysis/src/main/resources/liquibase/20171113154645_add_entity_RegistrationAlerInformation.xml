<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>
    <!-- 注册预警信息表结构-->
    <changeSet id="20171113154645" author="jianwei.wu">
		<preConditions onFail="MARK_RAN">
	        <not>
	            <tableExists tableName="T_REGISTRATION_ALERT_INFORMATION"/>
	        </not>
	    </preConditions>
        <createTable tableName="T_REGISTRATION_ALERT_INFORMATION" remarks="注册预警信息">
            <column name="ID" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="NAME" type="VARCHAR(120)" remarks="名称"/>
            <column name="JOB_NUMBER" type="VARCHAR(20)" remarks="学号/工号"/>
            <column name="COLLOGE_NAME" type="VARCHAR(20)" remarks="所属学院"/>
            <column name="GRADE" type="VARCHAR(20)" remarks="所属年级"/>
            <column name="CLASS_NAME" type="VARCHAR(50)" remarks="所属班级"/>
            <column name="WARNING_TYPE" type="VARCHAR(50)" remarks="预警类型"/>
            <column name="WARNING_LEVEL" type="VARCHAR(50)" remarks="预警等级"/>
            <column name="WARNING_CONDITION" type="VARCHAR(200)" remarks="预警条件"/>
            <column name="WARNING_TIME" type="DATETIME" defaultValueDate="${now}" remarks="预警时间"/>
            <column name="CREATED_BY" type="BIGINT" remarks="创建人ID"/>
            <column name="CREATED_DATE" type="DATETIME" defaultValueDate="${now}" remarks="创建时间"/>
            <column name="LAST_MODIFIED_BY" type="BIGINT" remarks="最后一次修改人ID"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME" defaultValueDate="${now}" remarks="最后一次修改时间"/>
            <column name="DELETE_FLAG" type="INT(4)" defaultValueNumeric="0" remarks="删除标记(0正常,1删除)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
