<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>
    <!-- 预警设置表结构-->
    <changeSet id="20171113154645" author="jianwei.wu">
		<preConditions onFail="MARK_RAN">
	        <not>
	            <tableExists tableName="T_ALARM_SETTINGS"/>
	        </not>
	    </preConditions>
        <createTable tableName="T_ALARM_SETTINGS" remarks="预警设置">
            <column name="ID" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="TYPE" type="VARCHAR(20)" remarks="预警类别"/>
            <column name="NAME" type="VARCHAR(120)" remarks="名称"/>
            <column name="ORG_ID" type="BIGINT" remarks="机构ID"/>
            <column name="SETUP_OR_CLOSE" type="INT(4)" remarks="开启状态(10:启用 ;20:关闭；)"/>
            <column name="INCLUSION_LEVEL" type="INT(4)" remarks="包含级别"/>
            <column name="CREATED_BY" type="BIGINT" remarks="创建人ID"/>
            <column name="CREATED_DATE" type="DATETIME" defaultValueDate="${now}" remarks="创建时间"/>
            <column name="LAST_MODIFIED_BY" type="BIGINT" remarks="最后一次修改人ID"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME" defaultValueDate="${now}" remarks="最后一次修改时间"/>
            <column name="DELETE_FLAG" type="INT(4)" defaultValueNumeric="0" remarks="删除标记(0正常,1删除)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
